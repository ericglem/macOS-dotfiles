extends plaintex

snippet mue "Unnumbered equation" bA
\[
	$1
\]
endsnippet

snippet alig "Unnumbered aligned equations" b
\begin{align*}
	$1 &= $2 \\\\
\end{align*}
endsnippet

snippet item "Itemize environment" b
\begin{itemize}
	\item $1
\end{itemize}
endsnippet

snippet enum "Enumerate environment" b
\begin{enumerate}
	\item $1
\end{enumerate}
endsnippet

snippet env "Generic environment" b
\begin{$1}$2
	$3
\end{$1}
endsnippet
